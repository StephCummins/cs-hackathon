{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.2",
    "description": "A plain text description",
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "all_frames": true,
          "js": ["main.js"],
          "run_at": "document_end"
        }
      ],
    "web_accessible_resources": [
        {
          "resources": [ "assets/shrek-mike-wazowski.gif"],
          "matches": ["*://*/*"]
        }
    ]
  }